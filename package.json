{
  "name": "projectile-sampling-distributions",
  "version": "1.1.0-dev.5",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/projectile-sampling-distributions.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "PROJECTILE_SAMPLING_DISTRIBUTIONS",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet",
      "phet-io"
    ],
    "phetLibs": [
      "projectile-data-lab",
      "bamboo"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsInteractiveDescription": true,
      "supportsSound": true
    },
    "screenNameKeys": [
      "PROJECTILE_DATA_LAB/screen.sampling"
    ],
    "phet-io": {
      "compareDesignedAPIChanges": true,
      "wrappers": [
        "phet-io-sim-specific/repos/projectile-sampling-distributions/wrappers/codap"
      ]
    },
    "published": true
  }
}